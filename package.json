    "scripts": {
        "build": "vite build --emptyOutDir",
        "start": "concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run serve\" \"npm run flask-server\"",
        "flask-server": "poetry run flask --app flask_app.app run --host=0.0.0.0",
        "flask-static-digest": "poetry run flask --app flask_app digest compile",
        "serve": "vite preview",
        "dev": "vite",
        "lint": "eslint . --ext .js,.vue --ignore-path .gitignore --fix vue/src"
    },
