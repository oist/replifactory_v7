{
    "name": "replifactory-ui",
    "version": "1.0.0",
    "description": "Ð¡ontrol a machine for bacterial growth experiments.",
    "private": true,
    "scripts": {
        "build": "run-script-os",
        "build:win32": "SET NODE_ENV=production && vue-cli-service build && npm run flask-static-digest",
        "build:default": "NODE_ENV=production vue-cli-service build && npm run flask-static-digest",
        "start": "run-script-os",
        "start:win32": "concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run serve:win32\" \"npm run flask-server\"",
        "start:default": "concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run serve\" \"npm run flask-server\"",
        "flask-server": "poetry run flask --app flask_app.app run --host=0.0.0.0",
        "flask-static-digest": "poetry run flask --app flask_app digest compile",
        "serve": "run-script-os",
        "serve:win32": "SET NODE_ENV=development && vue-cli-service serve",
        "serve:default": "NODE_ENV=development vue-cli-service serve",
        "lint": "vue-cli-service lint"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/catalin-rusnac/replifactory_v7.git"
    },
